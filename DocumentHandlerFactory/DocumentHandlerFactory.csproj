<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0.0-rc.6</Version>
    <Description>It allows us to perform document related operations via ftp or file system.</Description>
    <Authors>Kursat Sonmez</Authors>
    <Company>Kursat Sonmez</Company>
    <PackageId>DocumentHandlerFactory</PackageId>
    <RepositoryUrl>https://github.com/KursatSonmez/DocumentHandlerFactory</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>document-handler document-save doc-to-ftp</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
  </ItemGroup>

</Project>
